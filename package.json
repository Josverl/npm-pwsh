{
  "name": "get-powershell",
  "version": "0.0.4",
  "description": "Automatically downloads and extracts powershell, allowing you to use it in NPM scripts and node projects even with Linux and Mac developers who don't know or care about installing it.",
  "bin": {
    "pwsh": "./bin/pwsh"
  },
  "scripts": {
    "postinstall": "node ./out/npm_lifecycle_postinstall.js",
    "compile": "tsc -p ."
  },
  "author": "Andrew Bradley <cspotcode@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cspotcode/npm-get-powershell.git"
  },
  "homepage": "https://github.com/cspotcode/npm-get-powershell",
  "bugs": {
    "url": "https://github.com/cspotcode/npm-get-powershell/issues"
  },
  "keywords": [
    "PowerShell",
    "pwsh",
    "scripts",
    "shell",
    "npm"
  ],
  "dependencies": {
    "@types/unzipper": "^0.8.4",
    "cmd-shim": "^2.0.2",
    "get-stream": "^3.0.0",
    "mkdirp": "^0.5.1",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "tar": "^4.1.1",
    "tslib": "^1.8.1",
    "unzipper": "^0.9.2",
    "which": "^1.3.0"
  },
  "devDependencies": {
    "@types/get-stream": "^3.0.1",
    "@types/mkdirp": "^0.5.1",
    "@types/node": "^8.10.24",
    "@types/request": "^2.0.4",
    "@types/request-promise": "^4.1.38",
    "@types/tar": "^4.0.0",
    "@types/unzip": "^0.1.0",
    "@types/which": "^1.3.1",
    "ts-node": "^7.0.0",
    "typescript": "^3.0.1"
  },
  "files": [
    "out",
    "src",
    "bin",
    "tsconfig.json"
  ]
}
